{  
    "experiment" : {
        "scope": "examples_configs",
        "parameters" : {
            "lock_release_tout":120,
            "propagate":[
                {"in_sections" : ["explainers"],"params" : {"fold_id": 0, "retrain": false}},
                {"in_sections" : ["do-pairs/oracle"],"params" : {"fold_id": -1,"retrain": false}},
                {"in_sections": ["do-pairs/dataset"],"params": {"compose_man" : "config/snippets/datasets/padding_and_causality_and_centr_and_weights.json" }}
            ]
        }
    },
    "do-pairs":[ {
        "dataset" : {
          "class": "src.dataset.dataset_base.Dataset",
          "parameters": {
              "generator": {
                    "class": "src.dataset.generators.nci109.NCI109", 
                    "parameters": { "data_dir": "data/datasets/NCI109" }
              }
          } 
      },
        "oracle": {
          "class": "src.oracle.nn.torch.OracleTorch",
          "parameters": {
              "epochs": 550,
              "batch_size": 64,
              "optimizer": {
                  "class": "torch.optim.Adam",
                  "parameters": {
                      "lr":0.001 
                  }
                },
              "loss_fn": {
                  "class": "torch.nn.CrossEntropyLoss",
                  "parameters": {     
                    "reduction":"mean"
                  }
                },
              "model": { 
                "class": "src.oracle.nn.gcn.DownstreamGCN",
                "parameters": {"num_conv_layers":3,"num_dense_layers":3,"conv_booster":20,"linear_decay":0.2}
            } 
          }   
      }
    }],
    
    "explainers": [{"class": "src.explainer.generative.clear.CLEARExplainer","parameters":{ "epochs": 200, "lr": 0.001, "lambda_cfe": 0.1, "alpha": 0.4, "batch_size_ratio": 0.05 }}],
    "compose_mes" : "config/snippets/default_metrics.json",
    "compose_strs" : "config/snippets/default_store_paths.json"
}
